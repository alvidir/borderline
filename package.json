{
  "name": "borderline",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.1.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/query-string": "^6.3.0",
    "@types/unsplash-js": "^6.0.1",
    "fetch": "^1.1.0",
    "google-auth-library": "^6.0.6",
    "google-protobuf": "^3.12.4",
    "grpc": "^1.24.3",
    "grpc-tools": "^1.9.1",
    "grpc-web": "^1.2.0",
    "grpc_tools_node_protoc_ts": "^4.1.3",
    "http2": "^3.3.7",
    "node-fetch": "^2.6.0",
    "protobufjs": "^6.10.1",
    "protoc-gen-grpc": "^1.4.1",
    "protoc-gen-grpc-web": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.1.21",
    "react-scripts": "^3.4.3",
    "request": "^2.88.2",
    "typescript": "^3.9.7",
    "universal-cookie": "^4.0.3",
    "unsplash-js": "^6.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "proto-unsplash": "protoc -I=. ./proto/unsplash/*.proto --plugin ./node_modules/.bin/protoc-gen-grpc-web --js_out=import_style=commonjs:./src --grpc-web_out=import_style=commonjs,mode=grpcwebtext:./src",
    "proto-model": "protoc -I=. ./proto/model/*.proto --plugin ./node_modules/.bin/protoc-gen-grpc-web --js_out=import_style=commonjs:./src --grpc-web_out=import_style=commonjs,mode=grpcwebtext:./src",
    "proto-login": "protoc -I=. ./proto/login/*.proto --plugin ./node_modules/.bin/protoc-gen-grpc-web --js_out=import_style=commonjs:./src --grpc-web_out=import_style=commonjs,mode=grpcwebtext:./src",
    "proto": "npm run proto-unsplash && npm run proto-model && npm run proto-login"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "regenerator-runtime": "^0.13.7"
  }
}
